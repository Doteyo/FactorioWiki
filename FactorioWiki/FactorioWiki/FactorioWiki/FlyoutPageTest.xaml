<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FactorioWiki.FlyoutPageTest"
             xmlns:pages="clr-namespace:FactorioWiki"
            BackgroundColor="#333333"
            Title="Home">
  <FlyoutPage.Flyout>
    <pages:FlyoutPageTestFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
          <pages:MainPage BackgroundColor="#333333"/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>